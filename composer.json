{
    "name": "sabow/twilio-laravel",
    "description": "Package Laravel pour intégrer l'API Twilio et la vérification de numéro",
    "require": {
        "php": "*",
        "twilio/sdk": "^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Sabow\\TwilioLaravel\\": "src/"
        }
    },
    
    "extra": {
    "laravel": {
      "providers": [
        "Sabow\\TwilioLaravel\\Providers\\TwilioServiceProvider"
      ],
      "aliases": {
        "Twilio": "Sabow\\TwilioLaravel\\Facades\\Twilio"
      }
    }
  },
    "authors": [
        {
            "name": "SABOWA Ryan",
            "email": "screentapinc@gmail.com"
        }
    ],
    "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true
}
